let ItemsComponent = class ItemsComponent {
    constructor(modalctrl) {
        this.modalctrl = modalctrl;
        this.items = [];
    }
    ngOnInit() {
        this.getTitleInFirebase();
    }
    close(item) {
        setTimeout(() => {
            this.modalctrl.dismiss({
                'title': item
            });
        }, 200);
    }
    getTitleInFirebase() {
        this.afDB.list('Livraisons/').snapshotChanges(['child_added', 'child_removed', 'child_changed']).subscribe(actions => {
            actions.forEach(action => {
                this.items.push(action.payload.exportVal().title);
            });
        });
    }
};
