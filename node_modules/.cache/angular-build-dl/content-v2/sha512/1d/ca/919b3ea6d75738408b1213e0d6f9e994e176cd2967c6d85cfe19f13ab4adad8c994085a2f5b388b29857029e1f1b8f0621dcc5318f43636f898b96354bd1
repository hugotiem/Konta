/***/ "./src/app/tab3/new/new.component.ts":
/*!*******************************************!*\
  !*** ./src/app/tab3/new/new.component.ts ***!
  \*******************************************/
/*! exports provided: NewComponent */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "NewComponent", function() { return NewComponent; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "./node_modules/tslib/tslib.es6.js");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/__ivy_ngcc__/fesm2015/core.js");
/* harmony import */ var _ionic_angular__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @ionic/angular */ "./node_modules/@ionic/angular/__ivy_ngcc__/fesm2015/ionic-angular.js");
/* harmony import */ var _items_items_component__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ./items/items.component */ "./src/app/tab3/new/items/items.component.ts");
/* harmony import */ var _names_names_component__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ./names/names.component */ "./src/app/tab3/new/names/names.component.ts");






let NewComponent = class NewComponent {
    constructor(modalCtrl, popoverCtrl) {
        this.modalCtrl = modalCtrl;
        this.popoverCtrl = popoverCtrl;
        this.isAlreadyHere = false;
        if (this.date == undefined) {
            this.date = new Date().toISOString();
        }
        if (this.quantite == undefined) {
            this.quantite = 1;
        }
    }
    ngOnInit() { }
    close(save) {
        if (save) {
            this.modalCtrl.dismiss({
                'save': save,
                'key': this.key,
                'title': this.title,
                'price': this.price,
                'firstPrice': this.firstPrice,
                'km': this.km,
                'date': this.date,
                'name': this.name,
                'quantite': this.quantite
            });
        }
        else {
            this.modalCtrl.dismiss({
                'save': save
            });
        }
    }
    add() {
        this.quantite++;
    }
    remove() {
        if (this.quantite > 1) {
            this.quantite--;
        }
    }
    itemsModal() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const modal = yield this.modalCtrl.create({
                component: _items_items_component__WEBPACK_IMPORTED_MODULE_3__["ItemsComponent"],
                componentProps: {
                    afDB: this.afDB
                },
                cssClass: 'my-custom-class'
            });
            modal.onDidDismiss().then((data) => {
                if (data.data.title !== null) {
                    this.title = data.data.title;
                }
            });
            return yield modal.present();
        });
    }
    activated() {
        document.querySelector('#date-item').classList.toggle('no-input-feet');
    }
    namesPopover(ev) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            document.querySelector('#no-input').classList.toggle('no-input-feet');
            const popover = yield this.popoverCtrl.create({
                component: _names_names_component__WEBPACK_IMPORTED_MODULE_4__["NamesComponent"],
                cssClass: 'sort-popover',
                event: ev,
                mode: "md",
                translucent: true,
            });
            popover.onDidDismiss().then((data) => {
                if (data.data !== undefined) {
                    this.name = data.data.fromPopover;
                }
                document.querySelector('#no-input').classList.toggle('no-input-feet');
            });
            return yield popover.present();
        });
    }
    save() {
        if (this.isAlreadyHere) {
            this.modifyItemInFirebase(this.title, this.price, this.km, this.date, this.name, this.firstPrice, this.quantite);
        }
        else {
            this.addItemToFirebase(this.title, this.price, this.km, this.date, this.name, this.firstPrice, this.quantite);
        }
        this.close(true);
    }
    addItemToFirebase(title, price, distance, date, name, firstPrice, quantite) {
        this.afDB.list('Livraisons/').push({
            title: title,
            price: price,
            firstPrice: firstPrice,
            distance: distance,
            date: date,
            name: name,
            quantite: quantite
        });
    }
    modifyItemInFirebase(title, price, distance, date, name, firstPrice, quantite) {
        this.afDB.object('Livraisons/' + this.key + '/title/').set(title);
        this.afDB.object('Livraisons/' + this.key + '/price/').set(price);
        this.afDB.object('Livraisons/' + this.key + '/distance/').set(distance);
        this.afDB.object('Livraisons/' + this.key + '/name/').set(name);
        this.afDB.object('Livraisons/' + this.key + '/date/').set(date);
        this.afDB.object('Livraisons/' + this.key + '/firstPrice/').set(firstPrice);
        this.afDB.object('Livraisons/' + this.key + '/quantite/').set(quantite);
    }
};
NewComponent.ctorParameters = () => [
    { type: _ionic_angular__WEBPACK_IMPORTED_MODULE_2__["ModalController"] },
    { type: _ionic_angular__WEBPACK_IMPORTED_MODULE_2__["PopoverController"] }
];
NewComponent.propDecorators = {
    afDB: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"] }],
    isAlreadyHere: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"] }],
    key: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"] }],
    title: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"] }],
    price: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"] }],
    firstPrice: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"] }],
    km: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"] }],
