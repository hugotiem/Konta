 componentProps: {\n        afDB: this.afDB,\n        isAlreadyHere: true,\n        // KEY ITEM \n        key: this.key,\n        // ATTRIBUTS\n        title: this.title,\n        price: this.price,\n        firstPrice: this.firstPrice,\n        name: this.name,\n        date: this.date,\n        km: this.km,\n        quantite: this.quantite\n      },\n      cssClass: 'my-custom-class'\n    });\n\n    modal.onDidDismiss().then((data: any) => {\n      data = data.data;\n\n      if(data.save){\n        this.title = data.title;\n        this.price = data.price;\n        this.firstPrice = data.firstPrice;\n        this.name = data.name;\n        this.km = data.km;\n        this.quantite = data.quantite; \n      }     \n    })\n\n    return await modal.present();   \n  }\n\n  delete() {\n    this.afDB.list('Livraisons/').remove(this.key);\n    this.goBack();\n  }\n\n\n}\n"]}