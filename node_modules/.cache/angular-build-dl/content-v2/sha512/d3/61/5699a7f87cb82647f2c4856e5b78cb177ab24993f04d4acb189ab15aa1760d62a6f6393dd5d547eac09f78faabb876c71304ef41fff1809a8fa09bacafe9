y) => {\n      data = data.data;\n\n      if(data.save){\n        this.item.title = data.title;\n        this.item.price = data.price;\n        this.item.firstPrice = data.firstPrice;\n        this.item.name = data.name;\n        this.item.km = data.km;\n        this.item.quantite = data.quantite; \n        this.item.clientName = data.clientName;\n        this.item.type = data.type;\n      }     \n    })\n\n    return await modal.present();   \n  }\n\n  delete() {\n\n    if(this.infoFormule.length != 0){\n\n      for(let f of Object.keys(this.infoFormule)){\n        this.afDB.object('produits/' + this.infoFormule[f].tab.key + '/stock/').set(this.infoFormule[f].tab.quantite + this.infoFormule[f].quantite);\n      }\n    }\n    else if (this.infoProduit != undefined) {\n      this.afDB.object('produits/' + this.infoProduit[0].key + '/stock/').set(this.infoProduit[0].quantite + this.item.quantite);\n    }\n  \n    this.afDB.list('Livraisons/').remove(this.item.key);\n    this.goBack();\n  }\n\n\n}\n"],"sourceRoot":"webpack:///"}