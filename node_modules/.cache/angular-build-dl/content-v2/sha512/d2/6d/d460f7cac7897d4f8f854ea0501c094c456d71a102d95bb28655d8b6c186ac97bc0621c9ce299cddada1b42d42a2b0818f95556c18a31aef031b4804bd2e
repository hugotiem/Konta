let QuantiteComponent = class QuantiteComponent {
    constructor(modalCtrl) {
        this.modalCtrl = modalCtrl;
        this.disabled = false;
    }
    ngOnInit() {
    }
    close(bool) {
        if (bool) {
            this.modalCtrl.dismiss({
                'quantite': this.quantite
            });
        }
        else {
            this.modalCtrl.dismiss();
        }
    }
    add() {
        this.quantite++;
        this.disabled = false;
    }
    remove() {
        if (this.quantite > 1) {
            this.quantite--;
            if (this.quantite == 1) {
                this.disabled = true;
            }
        }
    }
};
