let DetailComponent = class DetailComponent {
    constructor(modalCtrl) {
        this.modalCtrl = modalCtrl;
        this.edit = false;
        this.disabled = false;
    }
    ngOnInit() { }
    close() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            yield this.modalCtrl.dismiss();
        });
    }
    update() {
        if (this.newName !== undefined) {
            this.item.name = this.newName;
        }
        if (this.newCategory !== undefined) {
            this.item.category = this.newCategory;
        }
        if (this.newPrice !== undefined) {
            this.item.price = this.newPrice;
        }
        if (this.newStock !== undefined) {
            this.item.stock = this.newStock;
        }
        for (let i in this.item) {
            if (i !== 'imgUrl') {
                this.afBD.object('produits/' + this.key + '/name/').set(this.item.name);
            }
        }
    }
    setEdit() {
        if (this.edit) {
            this.edit = false;
            document.querySelector('.delete-button').classList.toggle('activated-button');
            document.querySelectorAll('.chip').forEach(doc => {
                doc.classList.toggle('activated-chip');
            });
            setTimeout(() => {
                this.disabled = false;
            }, 500);
        }
        else {
            this.edit = true;
            this.disabled = true;
            setTimeout(() => {
                document.querySelector('.delete-button').classList.toggle('activated-button');
                document.querySelectorAll('.chip').forEach(doc => {
                    doc.classList.toggle('activated-chip');
                });
            }, 0);
        }
    }
};
