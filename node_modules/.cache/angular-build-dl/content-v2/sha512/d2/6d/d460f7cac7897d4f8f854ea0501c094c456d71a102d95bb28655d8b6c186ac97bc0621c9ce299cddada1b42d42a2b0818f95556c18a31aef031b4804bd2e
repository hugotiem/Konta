let AddComponent = class AddComponent {
    constructor(modalCtrl) {
        this.modalCtrl = modalCtrl;
        this.stock = 1;
        this.error = false;
    }
    ngOnInit() {
    }
    close() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            yield this.modalCtrl.dismiss();
        });
    }
    openQuantityModal() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const modal = yield this.modalCtrl.create({
                component: _quantite_quantite_component__WEBPACK_IMPORTED_MODULE_3__["QuantiteComponent"],
                componentProps: {
                    quantite: this.stock
                },
                cssClass: 'quantite-class',
                swipeToClose: true,
                mode: 'ios',
            });
            document.querySelector('#modal').classList.toggle('no-input-feet');
            modal.onWillDismiss().then((data) => {
                if (data.data !== undefined) {
                    this.stock = data.data.quantite;
                }
                document.querySelector('#modal').classList.toggle('no-input-feet');
            });
            return yield modal.present();
        });
    }
    addItemToFirebase() {
        console.log(this.title);
        if (this.empty(this.title) || this.empty(this.category) || this.empty(this.price) || this.empty(this.stock) || this.empty(this.firstPrice)) {
            this.error = true;
        }
        else {
            this.error = false;
            this.afBD.list('produits').push({
                name: this.title,
                category: this.category,
                price: this.price,
                firstPrice: this.firstPrice,
                stock: this.stock,
            });
            this.close();
        }
    }
    empty(obj) {
        if (obj == undefined) {
            return true;
        }
        return false;
    }
};
