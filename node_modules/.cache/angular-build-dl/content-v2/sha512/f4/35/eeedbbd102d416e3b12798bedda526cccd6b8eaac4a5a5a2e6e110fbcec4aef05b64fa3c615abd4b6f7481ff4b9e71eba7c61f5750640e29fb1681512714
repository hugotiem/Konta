// COMPONENTS FROM IONIC


// OWN COMPONENTS



let Tab2Page = class Tab2Page {
    constructor(router, afDB, popoverController, modalController) {
        this.router = router;
        this.afDB = afDB;
        this.popoverController = popoverController;
        this.modalController = modalController;
        this.items = [];
        this.getItemsFromFirebase();
    }
    doRefresh(event) {
        setTimeout(() => {
            event.target.complete();
        }, 1000);
    }
    presentPopover(ev) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const popover = yield this.popoverController.create({
                component: _popover_popover_component__WEBPACK_IMPORTED_MODULE_5__["PopoverComponent"],
                componentProps: {},
                cssClass: 'sort-popover',
                event: ev,
                translucent: true
            });
            return yield popover.present();
        });
    }
    presentModal(item) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const modal = yield this.modalController.create({
                component: _detail_detail_component__WEBPACK_IMPORTED_MODULE_6__["DetailComponent"],
                componentProps: {
                    item: {
                        name: item.name,
                        category: item.category,
                        price: item.price,
                        stock: item.stock,
                        imgUrl: '../assets/img/marlboro_gold.jpg'
                    },
                    key: item.key,
                    afBD: this.afDB
                },
                cssClass: 'my-custom-class'
            });
            return yield modal.present();
        });
    }
    presentAddModal() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const modal = yield this.modalController.create({
                component: _add_add_component__WEBPACK_IMPORTED_MODULE_7__["AddComponent"],
                componentProps: {
                    afBD: this.afDB
                },
                cssClass: ''
            });
            return yield modal.present();
        });
    }
    getItemsFromFirebase() {
        this.afDB.list('produits/').snapshotChanges(['child_added', 'child_moved']).subscribe(actions => {
            actions.forEach(action => {
                let item = action.payload.exportVal();
                this.items.push({
                    key: action.key,
                    name: item.name,
                    category: item.category,
                    price: item.price,
                    stock: item.stock
                });
            });
        });
    }
    delete(item) {
        this.afDB.list('produits/').remove(item.key);
    }
};
