let EditPage = class EditPage {
    constructor(navCtrl, modalController, app, route) {
        this.navCtrl = navCtrl;
        this.modalController = modalController;
        this.app = app;
        this.route = route;
        this.afDB = this.app.afDB;
        this.route.queryParams.subscribe(params => {
            this.key = params["key"];
            this.title = params["title"];
            this.price = params["price"];
            this.firstPrice = params["firstPrice"];
            this.name = params["name"];
            this.date = params["date"];
            this.km = params["km"];
            this.quantite = params["quantite"];
        });
    }
    ngOnInit() {
    }
    goBack() {
        this.navCtrl.navigateBack('tabs/tab3');
    }
    addNewExpModal() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const modal = yield this.modalController.create({
                component: _new_new_component__WEBPACK_IMPORTED_MODULE_3__["NewComponent"],
                componentProps: {
                    afDB: this.afDB,
                    isAlreadyHere: true,
                    // KEY ITEM 
                    key: this.key,
                    // ATTRIBUTS
                    title: this.title,
                    price: this.price,
                    firstPrice: this.firstPrice,
                    name: this.name,
                    date: this.date,
                    km: this.km,
                    quantite: this.quantite
                },
                cssClass: 'my-custom-class'
            });
            return yield modal.present();
        });
    }
    delete() {
        this.afDB.list('Livraisons/').remove(this.key);
        this.goBack();
    }
};
