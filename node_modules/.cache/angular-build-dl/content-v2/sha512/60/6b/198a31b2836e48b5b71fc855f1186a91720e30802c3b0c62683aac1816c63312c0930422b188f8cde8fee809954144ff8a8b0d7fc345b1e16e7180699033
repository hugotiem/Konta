ew Date().toISOString();\n  items: any = [];\n  infos: any = [];\n\n  total: number = 0;\n  private charges: number = 0;\n\n  admin: any = [\n    { name: 'Hugo', tab: {total: 0, rapport: 0} },\n    { name: 'Alexis', tab: {total: 0, rapport: 0} },\n    { name: 'Taheir', tab: {total: 0, rapport: 0} },\n    { name: 'Marvyn', tab: {total: 0, rapport: 0} },\n    { name: 'Gud', tab: {total: 0, rapport: 0} }\n\n  ]\n\n  selectedSlide: any;\n  segment = 0;\n  slidesOptions = {\n    initialSlide: 0,\n    slidesPerView: 1,\n    speed: 400\n  }\n\n  constructor(\n    private modalController: ModalController,\n    private app: AppComponent, \n    private afDB: AngularFireDatabase, \n    private router: Router, \n    private alertCtrl: AlertController, \n    private navCtrl: NavController) \n  {\n    this.afDB = this.app.afDB;\n    this.getStonksInFirebase();\n    this.getItemsFromFirebase();\n  }\n\n  async segmentChanged(ev) {\n    await this.selectedSlide.slideTo(this.segment);\n  }\n\n  async slideChanged(slides: IonSlides){\n    this.selectedSlide = slides;\n    slides.getActiveIndex().then(selectedIndex => {\n      this.segment = selectedIndex;\n      if(this.segment == 0){\n        this.pageName = false;\n        //slides.lockSwipeToPrev(true);\n        //slides.lockSwipeToNext(false);\n      }\n      else if (this.segment == 1){\n        this.pageName = true;\n      }\n      else {\n        //slides.lockSwipeToPrev(false);\n        //slides.lockSwipeToNext(true);\n      }\n    })\n    \n  }\n\n  async addNewExpModal() {\n    const modal = await this.modalController.create({\n      component: NewComponent,\n      componentProps: {\n        afDB: this.afDB,\n      },\n      cssClass: 'my-custom-class'\n    });\n    return await modal.present();   \n  }\n\n  goTo(item: any){\n\n    let infoProduit;\n    let infoFormule: any = [];\n\n    for(let f of Object.keys(this.infos)){\n      if(item.title == this.infos[f].title){\n        if(this.infos[f].formuleContent != undefined){\n          for(let i of Object.keys(this.infos[f].formuleContent)){\n            for(let j of Object.keys(this.infos)){\n              if(this.infos[j].title == this.infos[f].formuleContent[i].title && !this.infos[j].formule){\n                infoFormule.push({\n                  tab: this.infos[j],\n                  quantite: this.infos[f].formuleContent[i].quantite\n                });\n              }\n            }\n          }\n        }\n        if(infoFormule.length == 0){  \n          infoProduit = [];\n          infoProduit.push({\n            key: this.infos[f].key,\n            quantite: this.infos[f].quantite\n          })\n        }\n      }\n    }\n\n    let navData: NavigationExtras = {\n      queryParams: {\n        item: item,\n        infoProduit: infoProduit,\n        infoFormule : infoFormule\n      }\n    }\n    this.navCtrl.navigateForward('/tabs/tab3/edit', navData);\n  }\n\n  async confirmAlert() {\n    const alert = await this.alertCtrl.create({\n      cssClass: 'my-custom-class',\n      header: 'Confirmer',\n      message: 'Voulez-vous vraiment répartir le montant total avec tout le monde ?',\n      buttons: [\n        {\n          text: 'Annuler',\n          role: 'Cancel',\n        },\n        {\n          text: 'Confirmer',\n          handler: () => {\n            console.log(\"ok\");\n          }\n        }\n      ]\n    })\n    await alert.present();\n  }\n\n  getFuelPrice(km: number): number {\n    let price: number = 2;\n    if(km > 15) {\n      price = price + (((km-15)*1)/7);\n    } \n    else if (km == 0){\n      price = 0;\n    }\n    return price;\n  }\n\n  getPourcent(val: number, den: number) : number {\n\n    if(den == 0) {\n      den = 1;\n    }\n\n    return ((val/den) * 100);\n  }\n\n  getStonksInFirebase() {\n    this.afDB.list('Livraisons/').snapshotChanges(['child_added', 'child_removed', 'child_changed']).subscribe(actions => {\n\n      // INITIALISATIONS \n      this.items = [];\n      this.total = 0;\n      this.charges = 0;\n\n      for(let a of Object.keys(this.admin)){\n        this.admin[a].tab.rapport = 0;\n        this.admin[a].tab.total = 0;\n      }\n\n      // PARCOURT BDD FIREBASE\n      actions.forEach(action => {\n        let item = action.payload.exportVal();\n        this.total = this.total + ((+item.price) * item.quantite);\n\n        for(let n of Object.keys(this.admin)){\n          if(this.admin[n].name == item.name){\n            // CALCUL SOMME RAPPORTE\n            this.admin[n].tab.rapport = this.admin[n].tab.rapport + ((+item.price) * item.quantite);\n            // CALCUL SOMME GAGNE\n            this.admin[n].tab.total = this.admin[n].tab.total + ((+item.firstPrice) * item.quantite) + this.getFuelPrice(+item.distance);\n            // CALCUL DES CHARGES\n            this.charges = this.charges + ((+item.firstPrice) * item.quantite) + this.getFuelPrice(+item.distance);\n          }\n        }\n\n        this.items.push({\n          key: action.key,\n          title: item.title,\n          price: item.price,\n          firstPrice: item.firstPrice,\n          name: item.name,\n          date: item.date,\n          km: item.distance,\n          quantite: item.quantite,\n          clientName: item.clientName,\n          type: item.type,\n          keyProduit: item.key\n        });\n      });\n\n      // RAJOUTE LE BENEFICE AU TOTAL DE CHACUN\n      for(let n of Object.keys(this.admin)){\n        this.admin[n].tab.total = this.admin[n].tab.total + ((this.total - this.charges)/5);\n      }\n    });\n  }\n\n  getItemsFromFirebase(){\n    this.afDB.list('produits/').snapshotChanges(['child_added', 'child_moved', 'child_changed']).subscribe(actions => {\n      this.infos = [];\n      actions.forEach(action => {\n        let info = action.payload.exportVal();\n        this.infos.push({\n          key: action.key,\n          title: info.name,\n          quantite: info.stock,\n          formule: info.formule,\n          formuleContent: info.formuleContent,\n        })\n      })\n    })\n  }\n\n}\n"],"sourceRoot":"webpack:///"}